package ratan_practise;
class Test  {
    int eid;
    String ename;
    float esal;
    Test(int eid,String ename,float esal)
    { 
    	
    }
    
    void disp()
    {System.out.println("0-arg constructue");
    System.out.println(eid);
    System.out.println(ename);
    System.out.println(esal);
    }
    
	 public static void main(String[] args) {
		 Test t=new Test(111,"ratan",100000.50f);
		 t.disp();
		 Test t1=new Test(122,"ratna",20000.7f);
		 t1.disp();
		 
	
 }
}