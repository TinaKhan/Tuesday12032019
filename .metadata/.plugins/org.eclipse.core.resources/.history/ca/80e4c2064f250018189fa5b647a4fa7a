package foreachproject;

public class Foreachproject {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String Studentname[]= {"john","kabu","sabu","rabu"};
		for(int i=0;i<Studentname.length;i++)
		{System.out.println("Studentnames are :"+Studentname[i]);
		}
		
		int nam[]= {2,4,6,8};
		for(int ni:nam)
			System.out.println(ni);
		
	String name[]={"teshi","anthony","asher","hablu"};
	//String  studentName[] = {"John", "Singh",  "Aman", "Rahim"};
	for(String cats:name)
	System.out.println("kidsnames are :"+name);
	
			
	}
}

